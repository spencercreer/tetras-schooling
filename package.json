  "bugs": {
    "url": "https://github.com/spencercreer/tutor-templates/issues"
  },
  "homepage": "https://github.com/spencercreer/tutor-templates#readme",
  "devDependencies": {
    "concurrently": "^7.0.0"
  },
  "dependencies": {
    "apollo-server-express": "^3.6.3",
    "express": "^4.17.3",
    "graphql": "^16.3.0",
    "inquirer": "^8.2.0",
    "moment": "^2.29.1"
  }
}
